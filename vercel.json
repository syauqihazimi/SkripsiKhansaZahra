{
    "version": 2,
    "functions": {
      "public/index.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/index.php"
      }
    ],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "true",
      "APP_URL": "https://skripsikhansazahra.vercel.app/",
  
      "CACHE_DRIVER": "array",
      "LOG_CHANNEL": "stderr",
      "SESSION_DRIVER": "cookie",
      "VIEW_COMPILED_PATH": "/tmp"
    }
  }
  